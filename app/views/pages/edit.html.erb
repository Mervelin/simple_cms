<%= link_to("<< Back to List", pages_path, :class => 'back-link') %>

<div class="pages edit">
  <h2>Create page</h2>

  <%= form_for(@page, :url => page_path, :method => 'patch') do |f| %>

    <%= render(:partial => 'form', :locals => {:f => f}) %>

    <div class="form-buttons">
      <%= f.submit("Update page") %>
    </div>

  <% end %>
</div>
